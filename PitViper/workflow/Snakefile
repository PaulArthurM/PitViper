import pandas as pd

from snakemake.utils import min_version
min_version("5.24.0")

configfile: "config/configfile.yaml"

include: "rules/commons.smk"
include: "rules/mageck_mle_count.smk"
include: "rules/mageck_rra_count.smk"
include: "rules/bagel.smk"

rule all:
    input:
        get_pipeline_outputs


rule a:
    input:
        a="results/{token}/MAGeCK_MLE/{name}.sgrna_summary.txt",
        b="results/{token}/MAGeCK_MLE/MAGeCK_MLE_{name}.txt"
    output:
        "results/{token}/{name}_succeed.txt"
    shell:
        "echo '{wildcards.name} succeed.' > {output}"
